FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;

surfaceFormat raw;

surfaces
(
    airfoil
    {
        type            patch;
        patches         ( airfoil );
        patchName       airfoil;
    }
);

fields          ( p U V );


